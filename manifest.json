{
  "manifest_version": 2,
  "name": "RCR",
  "description": "React, Chrome, and Redux example",
  "version": "0.0.0.1",
  "background": {
    "scripts": ["web3.min.js", "event.js"],
    "persistent": true
  },
  "browser_action": {
    "default_title": "RCR Example",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.reddit.com/*"],
      "css": [],
      "js": ["jquery-slim.js", "content.js"]
    }
  ]
}
